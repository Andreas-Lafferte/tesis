@article{corvalan2013,
  title = {Class-{{Biased Electoral Participation}}: {{The Youth Vote}} in {{Chile}}},
  author = {Corvalan, Alejandro and Cox, Paulo},
  year = {2013},
  journal = {Latin American Politics and Society},
  volume = {55},
  number = {3},
  pages = {47--68},
  doi = {10.1111/j.1548-2456.2013.00202.x},
  abstract = {ABSTRACT There is no other country where the division between the old and young electorate is as striking as in Chile. For older voters, turnout exceeded, on average, 90 percent in 2009; for those aged less than 30, it fell below 30 percent. Using individual survey data from 2006, 2008, and 2010, this article studies the current socioeconomic composition of the Chilean young electorate. First, it shows that the young electorate is class-biased. Income is highly correlated with both registration and turnout even after controlling for education. Second, it presents evidence that class bias for the whole electorate has been increasing over time, due to generational replacement. The results are not promising for Chile's democracy in the years to come, since equal participation is worsening over time.}
}

