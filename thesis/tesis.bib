@article{daltonIndividualInstitutionalNexus2010,
  title = {The {{Individual}}\textendash{{Institutional Nexus}} of {{Protest Behaviour}}},
  author = {Dalton, Russell and Van Sickle, Alix and Weldon, Steven},
  year = {2010},
  month = jan,
  journal = {British Journal of Political Science},
  volume = {40},
  number = {1},
  pages = {51--73},
  issn = {0007-1234, 1469-2112},
  doi = {10.1017/S000712340999038X},
  abstract = {Political protest is seemingly a ubiquitous aspect of politics in advanced industrial societies, and its use may be spreading to less developed nations as well. Our research tests several rival theories of protest activity for citizens across an exceptionally wide range of polities. With data from the 1999\textendash{} 2002 wave of the World Values Survey, we demonstrate that the macro-level context \textendash{} levels of economic and political development \textendash{} significantly influences the amount of popular protest. Furthermore, a multi-level model examines how national context interacts with the micro-level predictors of protest activity. The findings indicate that contemporary protest is expanding not because of increasing dissatisfaction with government, but because economic and political development provide the resources for those who have political demands.}
}

