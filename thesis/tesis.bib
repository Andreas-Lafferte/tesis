@article{hicks1998,
  title = {Cooperation and {{Political Economic Performance}} in {{Affluent Democratic Capitalism}}},
  author = {Hicks, Alexander and Kenworthy, Lane},
  year = {1998},
  month = may,
  journal = {American Journal of Sociology},
  volume = {103},
  number = {6},
  pages = {1631--1672},
  publisher = {{The University of Chicago Press}},
  issn = {0002-9602},
  doi = {10.1086/231403},
  abstract = {Research on comparative political economic performance has traditionally followed two separate tracks, one concerned with collective economic gain (growth and efficiency) and the other focused on distribution and redistribution. Cooperative institutions offer a key to understanding cross\textendash national variation among the affluent capitalist democracies in both facets of political economic performance. These institutions cluster along two dimensions: neocorporatism and firm\textendash level cooperation. Pooled time\textendash series analysis for 18 nations over 1960\textendash\textendash 89 suggests that (1) neocorporatism is a major source of distributive/redistributive policies and outcomes and of several sources of collective gain; (2) firm\textendash level cooperation is a key contributor to economic growth.}
}

