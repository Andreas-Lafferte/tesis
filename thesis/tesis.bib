@article{wallaceFindingClassConsciousness2003,
  title = {Finding {{Class Consciousness In The New Economy}}},
  author = {Wallace, Michael and Junisbai, Azamat},
  year = {2003},
  month = jan,
  journal = {Research in Social Stratification and Mobility},
  volume = {20},
  pages = {385--421},
  issn = {02765624},
  doi = {10.1016/S0276-5624(03)20010-X},
  abstract = {It is axiomatic among Marxist scholars that class consciousness increases with rising levels of job insecurity and economic uncertainty. This paper examines the main factors affecting the development of class consciousness in the era of the new economy which is punctuated by downsizing, contingent work, and a general decline in employment security for many workers. We use data from the 1998 Indiana Survey of Work in Polarized Economy (ISWIPE) to examine factors affecting class consciousness. Results of the study suggest that the recent economic transformation has had considerable consequences for workers' class consciousness and that many of these effects vary in their impact by class.}
}

